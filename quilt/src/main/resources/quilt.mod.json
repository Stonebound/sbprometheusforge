{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${maven_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "Me!": "${mod_authors}"
      },
      "icon": "assets/sbprometheus/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.stonebound.sbprometheus.quilt.SbPrometheusQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "${quilt_quiltloader}"
      },
      {
        "id": "quilt_base",
        "version": "${quilt_quiltbase}"
      },
      {
        "id": "minecraft",
        "version": "${quilt_minecraft}"
      },
      {
        "id": "architectury",
        "version": "${quilt_architectury}"
      }
    ]
  },
  "mixin": [
    "sbprometheus.mixins.json"
  ]
}
